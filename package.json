{
  "name": "probabilistic-earley-parser",
  "version": "0.9.3",
  "description": "A parser for parsing Probabilistic Context Free Grammars",
  "main": "dist/index.js",
  "author": "Maarten Trompper",
  "license": "MIT",
  "repository": "git@github.com:digitalheir/probabilistic-earley-parser-javascript.git",
  "scripts": {
    "build": "npm run build:clean && npm run build:npm && npm run build:cp && npm run build:min",
    "build:clean": "rimraf dist",
    "build:min": "webpack -p",
    "build:npm": "tsc",
    "build:cp": "copyfiles .npmignore README.md package.json dist/",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:chart": "mocha -r ts-node/register test/**/chart.spec.ts",
    "test:grammar": "mocha -r ts-node/register test/**/grammar.spec.ts",
    "test:earley": "mocha -r ts-node/register test/**/earley.spec.ts"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "semiring": "^1.2.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "0.9.35",
    "@types/mocha": "^2.2.34",
    "rimraf": "^2.5.4",
    "chai": "^3.5.0",
    "copyfiles": "^1.0.0",
    "mocha": "^3.2.0",
    "ts-loader": "^1.3.3",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "yargs": "latest"
  }
}
